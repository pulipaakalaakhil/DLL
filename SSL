#include<stdio.h>
#include<stdlib.h>

struct node
{
	int data;
	struct node *prev;
	struct node *next;
}*n,*head,*tail;

void ins_beg(int num)
{
	n=(struct node*)malloc(sizeof(struct node));
	n->data=num;
	n->next=NULL;
	if(head==NULL)
	{
		head=n;
		tail=n;
	}
	else
	{
		n->next=head;
		head=n;
	}
}
void ins_end(int num)
{
	n=(struct node*)malloc(sizeof(struct node));
	n->data=num;
	n->next=NULL;
	if(head==NULL)
	{
		head=n;
		tail=n;
	}
	else
	{
		tail->next=n;
		tail=n;
		n->next=NULL;
	}
}
void ins_mid(int num,int mid)
{
	struct node *t;
	n=(struct node*)malloc(sizeof(struct node));
	n->data=num;
	n->next=NULL;
	for(t=head;t->next!=NULL;t=t->next)
	{
		if(t->next->data==mid)
		{
			n->next=t->next;
			t->next=n;
			break;
		}
	}
}
void del_beg()
{
	struct node *t;
	t=head;
	head =t->next;
	free(t);
}
void del_end()
{
	struct node *t,*tp;
	t=head;
	while(t->next!=NULL)
	{
		tp=t;
		t=t->next;
	}
	tail=tp;
	tail->next=NULL;
	free(t);
}
void del_mid(int mid)
{
	struct node *t,*tp;
	t=head;
	while(t->next!=NULL)
	{
		if(t->next->data==mid)
		{
			tp=t;
			t=t->next;
			tp->next=t->next;
			free(t);
			break;
		}
		tp=t;
		t=t->next;
	}
}
void display()
{
	struct node *t;
	t=head;
	while(t!=NULL)
	{
		printf("%d ",t->data);
		t=t->next;
	}
	printf("\n");
}
void search(int num)
{
	struct node *t4;
	t4=head;
	while(t4!=NULL)
	{
		if(t4->data==num)
		{
			printf("\nfound");
			break;
		}
		t4=t4->next;
	
	if(t4==NULL)
	{
		printf("\nnot found");
	}
}
}
		
	int main()
	{
		
		printf("192373024_Akhilkumar\n");
    ins_beg(11);
    ins_beg(22);
    ins_beg(88);
    ins_beg(20);
    ins_end(37);
    ins_end(49);
    ins_end(45);
    ins_end(90);
    ins_mid(25, 35);
    printf("The created linked list is: \n");
    display();
    del_beg();
    del_end();
    del_mid(38);
    printf("\n After deletion, the linked list is: \n");
    display();
    printf("\n Searching the given element: \n");
search(11);

	}
